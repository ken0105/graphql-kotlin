type Query {
    olympics(yearFilter: Int): Result
}

type Mutation {
    addOlympic(country: String!, year: Int!): Result
    deleteOlympic(year: Int!): Result!
}

type Subscription{
    subscribeOlympics: Olympic
}

type Result {
    status: Status!
    olympics: [Olympic]
}

type Olympic {
    year: Int!
    country: String!
}

type Status {
    code: Int!,
    message: String!
}